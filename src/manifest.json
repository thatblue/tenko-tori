{
  "name": "Tenko-Tori",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Character counter for esa.io (v2 Editor)",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [{
    "matches": ["https://*.esa.io/posts/*/edit", "https://*.esa.io/posts/new"],
    "js": [
      "lib/jquery-3.4.1.min.js",
      "main.js"
    ]
  }]
}